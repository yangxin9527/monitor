{
  "name": "monitor",
  "version": "0.0.1-beta",
  "description": "Front End buried point",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && webpack --mode production",
    "start": "webpack serve --mode development --open",
    "server": "nodemon server/index ",
    "test": "jest",
    "lint": "eslint --fix",
    "build1": "rm -rf ./dist/js && webpack --config config/webpack.pro.config.js && node write.js  && conventional-changelog -p angular -i ./CHANGELOG.md -s",
    "dist-js": "rm -rf ./dist/js && webpack --config config/webpack.pro.config.js && node write.js && conventional-changelog -p angular -i ./CHANGELOG.md -s",
    "dev-js": "rm -rf ./dist/js && webpack --config config/webpack.pro.config.js --env.pro=dev && node write.js  && conventional-changelog -p angular -i ./CHANGELOG.md -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yangxin9527/monitor.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/yangxin9527/monitor/issues"
  },
  "homepage": "https://github.com/yangxin9527/monitor#readme",
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "body-parser": "^1.19.0",
    "chalk": "^2.4.2",
    "compression-webpack-plugin": "^3.0.0",
    "es3ify-loader": "^0.2.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "jest": "^24.8.0",
    "mongoose": "^5.6.2",
    "webpack": "latest",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0"
  },
  "devDependencies": {
    "@rrweb/types": "^2.0.0-alpha.13",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "latest",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "latest",
    "eslint": "^8.0.0",
    "eslint-webpack-plugin": "latest",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "html-webpack-plugin": "latest",
    "husky": "^3.0.3",
    "lint-staged": "^9.2.1",
    "mini-css-extract-plugin": "latest",
    "nodemon": "^3.1.4",
    "rimraf": "^6.0.1",
    "rrweb": "^2.0.0-alpha.4",
    "style-loader": "latest",
    "terser-webpack-plugin": "^5.3.10",
    "ts-loader": "latest",
    "typescript": "^5.5.4",
    "webpack-cli": "latest",
    "webpack-dev-server": "^5.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "testMatch": [
      "**/test/unit/*.spec.(js|jsx|ts|tsx)|**/__test__/*.(js|jsx|ts|tsx)"
    ],
    "testURL": "http://localhost/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**/*.js",
      "!**/node_modules/**"
    ]
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node git-hooks/verify-commit-msg.js"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  }
}
